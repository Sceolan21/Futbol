        android:theme="@style/AppTheme">
        <activity android:name=".Futbol">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Fragmentos"
            android:label="@string/app_name"
            android:parentActivityName=".Futbol" >
        </activity>
        <activity
            android:name=".Fragmento3"
            android:label="@string/app_name"
            android:parentActivityName=".Futbol" >
        </activity>
        <activity
            android:name=".Fragmento2"
            android:label="@string/app_name"
            android:parentActivityName=".Futbol" >
        </activity>
    </application>

</manifest>

